        if (e.which != 13) {
            $("hdnCIRNumber").val("0");
        }

        if ($("#ddlReportType > option:selected").val() == "1") {
            if ($("#txtCIRNumber").val() == "")
                $("hdnCIRNumber").val("0");

            var CIRDetails = {
                txt: $("#txtNumber").val(),
                refId: $('#ddlReportType').val(),
                viewType: "26"
            };

            $("#txtNumber").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: "@Url.Action("GetSearchDetails", "CIR")",
                        data: JSON.stringify(CIRDetails),
                        dataType: "json",
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        success: function (data) {
                            if (data.length != 0) {
                                response($.map(data, function (item) {

                                    if (data.indexOf(item) === -1) {
                                        return { label: "No Results." }
                                    } else {
                                        return {
                                            label: item.TextValue,
                                            val: item.Id
                                        }
                                    }

                                }))
                            }
                            else {
                                return { label: "No Results." }
                            }
                        },
                        error: function (response) {

                        },
                        failure: function (respose) {

                        }
                    });
                },
                minlength: 1,
                select: function (e, i) {
                    $("#hdnNumber").val(i.item.val);
                    $("#txtNumber").val(i.item.label);
                }
            });
        }
        else {
            if ($("#txtNumber").val() == "")
                $("hdnNumber").val("0");

            var CIRDetails = {
                txt: $("#txtNumber").val(),
                refId: $('#ddlReportType').val(),
