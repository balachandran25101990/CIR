@{
    ViewBag.Title = "DetailReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">
    var GetSubMasterCollectionSpecificationUrl = "@Url.Action("GetMasterCollectionSpecification", "Master")";
    var GetMasterCollectionUrl = "@Url.Action("GetMasterCollection", "Master")";
    var GetdepartmentUrl = "@Url.Action("GetDepartment", "Master")";
    var GetStatusUrl = "@Url.Action("GetStatus", "Admin")";
    var GetTurbineUrl = "@Url.Action("GetTurbine", "Master")";
    var GetDetailReportDetailsUrl = "@Url.Action("GetDetailReportDetails", "Report")";
    var GetCIRDetailsUrl = "@Url.Action("GetCIRDetails", "CIR")";
    var GetDesignationUrl = "@Url.Action("GetDesignation", "Master")";
    var GetDownloadUrl = "@Url.Action("DownloadExcel", "Report")";
    var GetClearDetailReptSession = "@Url.Action("SessionClearDetailRpt", "Report")";
    var GetLogFileUrl = "@Url.Action("LogFile", "Admin")";
    var GetAccessUrl = "@Url.Action("GetPageAccess", "Admin")";
</script>
<script src="~/Scripts/jquery.table2excel.js"></script>
<div class="col-lg-12 pad-0" ng-controller="DetailReport_OperController">
    <div class="content-part" style="min-height:90vh;">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <h5 class="heading-h">
                    Detail Report
                </h5>
            </div>
            <div class="row mar-0 fomr-div">
                <div class="row mar-0 fomr-div">
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Report Type</span><span style="color:red;padding:5px;">*</span>
                        <select id="ddlReportType" ng-model="RptType" ng-change="ChangeData()">
                            <option value="1" selected="selected">CIR</option>
                            <option value="2">FSR</option>
                        </select>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>CIR/FSR Number</span>
                        <input type="hidden" id="hdnNumber" value="0" />
                        <input type="text" id="txtNumber" ng-model="CIROrCIMNumber" />
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>State</span>
                        <select id="ddlState" ng-model="StateSelect" ng-change="SiteDetails()">
                            <option value="0">-- Select All --</option>
                            <option ng-repeat="option in StateArray" ng-selected="0" value="{{option.MCId}}">{{option.Name}}</option>
                        </select>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Site</span>
                        <select id="ddlSite" ng-model="SiteSelect" ng-change="TurbineDetails()">
                            <option value="0">-- Select All --</option>
                            <option ng-repeat="option in SiteArray" value="{{option.MCId}}">{{option.Name}}</option>
                        </select>
                    </div>
                </div>
                <div class="row mar-0 fomr-div">
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Turbine Type</span>
                        <select id="ddlTurbineType" ng-model="TurbineTypeSelect" ng-change="ChangeData()">
                            <option value="0">-- Select All --</option>
                            <option ng-repeat="option in TurbineTypeArray" value="{{option.MCId}}">{{option.Name}}</option>
                        </select>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Functional System</span>
                        <select id="ddlDepartment" ng-model="FunctionalSystemSelect" ng-change="ChangeData()">
                            <option value="0">-- Select All --</option>
                            <option ng-repeat="option in FunctionalSystemArray" value="{{option.MCId}}">{{option.Name}}</option>
                        </select>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Designation</span>
                        <select id="ddlDesignation" ng-model="DesignationSelect" ng-change="ChangeData()">
                            <option value="0">-- Select All --</option>
                            <option ng-repeat="option in DesignationArray" value="{{option.DsgId}}">{{option.Designation}}</option>
                        </select>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Status</span>
                        <select id="ddlStatus" ng-model="StatusSelect" ng-change="ChangeData()">
                            <option value="0">-- Select All --</option>
                            <option ng-repeat="option in StatusArray" value="{{option.StsId}}">{{option.Status}}</option>
                        </select>
                    </div>
                </div>
                <div class="row mar-0 fomr-div">
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Turbine</span> @*<select id="ddlTurbine" ng-model="TurbineSelect" ng-change="ChangeData()">
                            <option value="0">-- Select All --</option>
                            <option ng-repeat="option in TurbineArray" value="{{option.TbnId}}">{{option.TurbineId}}</option>
                            </select>*@
                        <input type="text" id="txtTurbine" ng-model="txtTurbine" />
                        <input type="hidden" id="hfTurbine" value="0" />
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Date From</span>
                        <input type="text" id="txtDateFrom" class="datepickerFormat" ng-model="DateFrom" ng-change="ChangeData()" onkeypress="return Enabled(event)" />
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Date To</span>
                        <input type="text" id="txtDateTo" class="datepickerFormat" ng-model="DateTo" ng-change="ChangeData()" onkeypress="return Enabled(event)" />
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>WO Number</span>
                        <input type="hidden" id="hdnWONumber" />
                        <input type="text" id="txtWONumber" ng-model="WONumber" ng-change="ChangeData()" />
                    </div>
                </div>
                <div class="row mar-0 fomr-div">
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <span>Employee</span>
                        <input type="hidden" value="0" id="hdnEmployeeId" />
                        <input type="text" id="txtEmployee" maxlength="200" ng-model="Employee" />
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12" style="margin-top: 15px;">
                        <span class="checkbox" style="margin-left: 20px;">
                            <input type="checkbox" ng-model="IsFileHaving" ng-true-value="'1'" ng-false-value="'0'"
                                   checked="checked" id="chkFile" style="width: auto; margin-top: -2px;" />With
                            File
                        </span>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12" style="margin-top: 11px;">
                        <a href="#" class="btn btn-default SearchButton" ng-click="Search()">Search</a>
                        <a href="#" class="btn btn-default ClearButton" ng-click="Clear()" style="margin-left:0px;">Clear</a>
                        <a href="#" class="btn btn-default ClearButton" id="btnExcelDownLoad" ng-click="DownloadDetail()" ng-show="Download && ReportDetailsArray.length > 0" style="margin-left:12px;">Excel DownLoad</a>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-xs-12 col-sm-6 fomr-div">
                </div>
                <div class="col-lg-12 col-md-12 col-xs-12 col-sm-6 fomr-div" ng-show="ReportDetailsArray.length > 0">
                    <div class="input-group col-xs-6 col-sm-6 col-lg-6 col-md-6" style="margin-bottom: 15px;">
                        <div class="input-group-addon"><i class="fa fa-search"></i></div>
                        <input type="text" class="form-control" placeholder="Search Details" ng-model="searchDetail" style="height: 35px;z-index:1;">
                    </div>
                    <div>
                        <div style="overflow-x:scroll;">
                            <table cellpadding="0" cellspacing="0" class="table table-hover table-responsive">
                                <thead style="border-top: 1px solid #dddddd;border-bottom: 2px solid #dddddd;">
                                    <tr>
                                        <td ng-click="sortType = 'SlNo'; sortReverse = !sortReverse" style="cursor:pointer;min-width:50px; text-align:center;">
                                            <a href="#" style="text-decoration: none; color: #6b6b6b; font-weight: bold;">
                                                SlNo
                                                <span ng-show="sortType == 'Slno' && !sortReverse" class="fa fa-caret-down"></span>
                                                <span ng-show="sortType == 'Slno' && sortReverse" class="fa fa-caret-up"></span>
                                            </a>
                                        </td>
                                        <td ng-click="sortType = 'CNumber'; sortReverse = !sortReverse" style="cursor:pointer;min-width:200px;">
                                            <a href="#" style="text-decoration: none;  color: #6b6b6b; font-weight: bold;">
                                                CIR/ FSR Number
                                                <span ng-show="sortType == 'CNumber' && !sortReverse" class="fa fa-caret-down"></span>
                                                <span ng-show="sortType == 'CNumber' && sortReverse" class="fa fa-caret-up"></span>
                                            </a>
                                        </td>
                                        <td ng-click="sortType = 'Turbine'; sortReverse = !sortReverse" style="cursor:pointer;min-width:80px;">
                                            <a href="#" style="text-decoration: none; color: #6b6b6b; font-weight: bold;">
                                                Turbine
                                                <span ng-show="sortType == 'Turbine' && !sortReverse" class="fa fa-caret-down"></span>
                                                <span ng-show="sortType == 'Turbine' && sortReverse" class="fa fa-caret-up"></span>
                                            </a>
                                        </td>
                                        <td ng-click="sortType = 'State'; sortReverse = !sortReverse" style="cursor:pointer;min-width:100px;">
                                            <a href="#" style="text-decoration: none; color: #6b6b6b; font-weight: bold;">
                                                State
                                                <span ng-show="sortType == 'State' && !sortReverse" class="fa fa-caret-down"></span>
                                                <span ng-show="sortType == 'State' && sortReverse" class="fa fa-caret-up"></span>
                                            </a>
                                        </td>
                                        <td ng-click="sortType = 'TSite'; sortReverse = !sortReverse" style="cursor:pointer;min-width:150px;">
