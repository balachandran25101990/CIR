<script type="text/javascript">
    function fnPrint() {

        $("#div").css("display", "none");

        $("#divPhotos").css("display", "none");
        $("#divPhotosP").css("display", "");
        $("#divFiles").css("display", "none");
        $("#divFilesP").css("display", "");
        window.print();
        $("#div").css("display", "");
        $("#divPhotos").css("display", "");
        $("#divPhotosP").css("display", "none");
        $("#divFiles").css("display", "");
        $("#divFilesP").css("display", "none");

     //   $('a').contents().wrap();
        
    }

    function fnShowImageBigger(obj) {
        var valueInHtml = obj.innerHTML;
        $("#modalBody").html(valueInHtml);

        $("#myModal").modal('show');
        $(".modal-dialog").css("width", "100%");
    };

    function fnLoadGif() {
        $("#divLoading").css("display", "");
    }

    function fnRemoveGif() {
        $("#divLoading").css("display", "none");
    }

    
    //$(document).ready(function () {
